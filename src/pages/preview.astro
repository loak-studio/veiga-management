---
import { StoryBlokRender } from "../components/StoryBlokRender";
import Main from "../layouts/Main.astro";
const metatags = {
  title: "Preview",
  description: "",
  image: "",
};
---

<script>
  setTimeout(() => {
    const items = Array.from(document.querySelectorAll('*')) as HTMLElement[]
    items.forEach((item) => {
      item.style.opacity = "1";
    });
  }, 1000);
</script>

<Main meta={metatags}>
  <StoryBlokRender token={import.meta.env.STORYBLOK_TOKEN} client:only />
</Main>
